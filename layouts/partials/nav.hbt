<nav id="{{id}}" class="{{class}}">
  <ul>
    {{#each nav as |item|}}
      {{#if (and item (not item.disabled))}}
        <li class="{{item.type}}{{#if (child-of ../../current item.href)}}current{{/if}}"><a href="{{link item.href}}" target="{{item.target}}" title="{{item.description}}">{{item.title}}</a></li>
      {{/if}}
    {{/each}}
  </ul>
</nav>